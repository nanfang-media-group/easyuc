{
    "name": "southcn/easyuc",
    "require": {
        "php": ">=7.1.0",
        "abelhalo/api-proxy": "^1.6",
        "nanfang-media-group/ticker-timer": "dev-master",
        "southcn/private-api": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nanfang-media-group/private-api.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "SouthCN\\EasyUC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SouthCN\\EasyUC\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SouthCN\\EasyUC\\EasyUCServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
